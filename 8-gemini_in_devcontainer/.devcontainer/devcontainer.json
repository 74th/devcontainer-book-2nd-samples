{
    "name": "caludecode",
    "build": {
        "dockerfile": "./Dockerfile",
    },
    "features": {},
    "remoteEnv": {},
    "mounts": [
        "source=${localEnv:HOME}/.llm/gemini/.gemini,target=/home/node/.gemini,type=bind,consistency=cached",
        "source=${localEnv:HOME}/.llm/gemini/.config/configstore,target=/home/node/.config/configstore,type=bind,consistency=cached"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                // "Google.geminicodeassist",
                "Google.gemini-cli-vscode-ide-companion"
            ]
        }
    }
}